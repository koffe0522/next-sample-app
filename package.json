{
  "name": "app",
  "version": "1.0.0",
  "description": "Next sample App",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "next": "next ./client",
    "dev": "nodemon",
    "serve": "cross-env NODE_ENV=production firebase serve",
    "deploy": "firebase deploy",
    "deploy-rules": "firebase deploy --only firestore:rules,storage:rules",
    "preserve": "yarn build-public && yarn build-functions && yarn build-client && yarn copy-deps && yarn install-deps && yarn test",
    "build": "rm -rf ./client/.next && tsc --project tsconfig.server.json && next build ./client",
    "build-client": "next build ./client",
    "build-public": "cpx \"client/public/**/*.*\" \"dist/public\" -C",
    "build-functions": "tsc --project ./client/functions",
    "typecheck-app": "tsc --project ./client",
    "copy-deps": "cpx \"*{package.json,package-lock.json,yarn.lock}\" \"dist/functions\" -C",
    "install-deps": "cd \"dist/functions\" && yarn install",
    "test": "npm run start-firestore & jest",
    "start-firestore": "firebase serve --only firestore",
    "clear": "rm -r dist && yarn run preserve",
    "refresh": "yarn cache clean && rm -r node_modules yarn.lock && yarn install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^4.2.1",
    "@material-ui/styles": "^4.2.1",
