{
    "presets": [
      "next/babel",
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "6"
          }
        }
      ]
    ],
    "env": {
      "test": { 
        "presets": [ "env", "next/babel" ]
      },
    },
    "plugins": [
        ["module-resolver", {
          "alias": {
            "app": "./"
          }
        }],
        "@babel/plugin-proposal-object-rest-spread",
        "@babel/plugin-transform-react-jsx"
      ],
}